{
  "name": "@edx/paragon",
  "version": "0.0.0-development",
  "description": "Accessible, responsive UI component library based on Bootstrap.",
  "main": "themeable/index.js",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/edx/paragon.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build-storybook": "build-storybook",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage",
    "deploy-storybook": "storybook-to-ghpages",
    "deploy-storybook-ci": "storybook-to-ghpages --ci",
    "gc": "commit",
    "is-es5": "es-check es5 ./themeable/*.js ./static/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "snapshot": "jest --updateSnapshot",
    "start": "start-storybook -p 6006",
    "test": "jest --coverage",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "airbnb-prop-types": "^2.12.0",
    "classnames": "^2.2.6",
    "email-prop-type": "^3.0.0",
    "font-awesome": "^4.7.0",
    "mailto-link": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-proptype-conditional-require": "^1.0.4",
    "react-responsive": "^6.1.1",
    "sanitize-html": "^1.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.0.1",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-centered": "^5.0.1",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.1",
    "@storybook/addon-storyshots": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "es-check": "^5.0.0",
    "eslint-config-edx": "^4.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "storybook-readme": "^4.0.5",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.md?$": "markdown-loader-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "setupFiles": [
      "./src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/setupTest.js",
      "src/index.js",
      "/tests/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@edx/paragon)/).*/"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not ie < 11"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  }
}
