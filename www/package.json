{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@^1.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "css": "^3.0.0",
    "gatsby": "^3.0.1",
    "gatsby-plugin-gatsby-cloud": "^2.0.0",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-mdx": "^2.1.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-page-creator": "^3.1.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sass": "^4.1.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-react-docgen": "^6.1.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "node-sass": "^5.0.0",
    "prism-react-renderer": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-live": "^2.2.3"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "build:with-theme": "THEME=npm:@edx/brand-edx.org@1.x npm run install-theme && gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "install-theme": "npm install \"@edx/brand@${THEME}\" --no-save",
    "develop:with-theme": "THEME=npm:@edx/brand-edx.org@1.x npm run install-theme && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --ext .js --ext .jsx .",
    "start": "npm run develop",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
